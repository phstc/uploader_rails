<!DOCTYPE html>
<html>
<head>
	<title>Super Uploads</title>
	<%= stylesheet_link_tag    "application", :media => "all" %>
	<%= javascript_include_tag "application" %>
	<%= csrf_meta_tags %>
</head>
<body>
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container">
				<%= link_to "Super Uploads", root_path, class: "brand" %>
				<div class="nav-collapse">
					<ul class="nav">
						<li class="dropdown">
							<a href="#" class="dropdown-toggle" data-toggle="dropdown">Uploads <b class="caret"></b></a>
							<ul class="dropdown-menu">
								<li><%= link_to "All Uploads", uploads_path %></li>
								<li><%= link_to "New Upload", new_upload_path %></li>
							</ul>
						</li>
					</ul>
				</div><!-- /.nav-collapse -->
			</div>
		</div><!-- /navbar-inner -->
	</div>
	<div class="content">
		<%= yield %>
	</div>
</body>
</html>
